{
  "expo": {
    "name": "Sasuke's Path",
    "slug": "sasukes-path",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#191c26"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.augusto.sasukepath",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "Permitir que o Sasuke's Path acesse sua c창mera para usar a Vis찾o do Sharingan.",
        "NSPhotoLibraryAddUsageDescription": "Permitir que o Sasuke's Path salve imagens na sua galeria.",
        "NSSpeechRecognitionUsageDescription": "Allow $(PRODUCT_NAME) to use speech recognition.",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to use the microphone."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#191c26"
      },
      "package": "com.augusto.sasukepath",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Permitir que o Sasuke's Path acesse sua c창mera para usar a Vis찾o do Sharingan."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Permitir que o Sasuke's Path salve imagens na sua galeria.",
          "savePhotosPermission": "Permitir que o Sasuke's Path salve imagens na sua galeria.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-localization",
      "expo-speech-recognition"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "eas": {
        "projectId": "4bc0066d-1263-4cd0-8e80-96fe28859750"
      }
    },
    "owner": "auggie22"
  }
}
