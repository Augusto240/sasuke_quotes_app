{
  "expo": {
    "name": "Sasuke's Path",
    "slug": "sasukes-path",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#191c26"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#191c26"
      },
      "package": "com.augusto.sasukepath"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Permitir que o Sasuke's Path acesse sua câmera para usar a Visão do Sharingan."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Permitir que o Sasuke's Path salve imagens na sua galeria.",
          "savePhotosPermission": "Permitir que o Sasuke's Path salve imagens na sua galeria.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  }
}